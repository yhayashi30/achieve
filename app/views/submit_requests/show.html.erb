<div class="child row">
  <div class="wrapper col-md-6 col-md-offset-3 col-sm-10 col-sm-offset-1">
    <p id="notice"><%= notice %></p>
    <p class="left">
      <strong>ユーザ:</strong>
      <%= @submit_request.user.name %>
    </p>
    <p class="left">
      <strong>タスク:</strong>
      <%= @submit_request.task.title %>
    </p>
    <p class="left">
      <strong>メッセージ:</strong>
      <%= @submit_request.message %>
    </p>
    <p class="left">
      <strong>依頼したユーザ:</strong>
      <%= @submit_request.request_user_id %>
    </p>
    <p class="left">
      <strong>ステータス:</strong>
      <%= @submit_request.status %>
    </p>
    <%= link_to '修正', edit_submit_request_path(@submit_request), class: 'btn btn-default' %> |
    <%= link_to '戻る', submit_requests_path, class: 'btn btn-default' %>
  </div>
</div>
