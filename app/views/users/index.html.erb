<div class="container">
  <h2>ユーザー</h2>
  <% @users.each do |user| %>
      <div class="container-child col-md-2 col-sm-4 col-xs-6 ">
        <%= link_to profile_img(user), detail_user_path(user.id), method: :post %>
        <%= user.email %>
        <%= render 'follow_form', user: user %>
      </div>
  <% end %>
</div>